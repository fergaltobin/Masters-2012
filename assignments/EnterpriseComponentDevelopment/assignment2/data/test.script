CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE MEMORY TABLE AGENT(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,NAME VARCHAR(255))
CREATE MEMORY TABLE CLUB(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,NAME VARCHAR(255),CAPACITY INTEGER NOT NULL,TICKET_PRICE FLOAT NOT NULL,TITLE VARCHAR(255),DIV BIGINT)
CREATE MEMORY TABLE CLUB_AGENT(CLUBSS_ID BIGINT NOT NULL,AGENTS_ID BIGINT NOT NULL,PRIMARY KEY(CLUBSS_ID,AGENTS_ID),CONSTRAINT FKDC04563C7D5922FD FOREIGN KEY(CLUBSS_ID) REFERENCES CLUB(ID),CONSTRAINT FKDC04563C3E46166E FOREIGN KEY(AGENTS_ID) REFERENCES AGENT(ID))
CREATE MEMORY TABLE COMPANY(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,NAME VARCHAR(255))
CREATE MEMORY TABLE CONTRACT(AGENTID BIGINT NOT NULL,PLAYERID BIGINT NOT NULL,VALUE FLOAT NOT NULL,PLAYER BIGINT,PRIMARY KEY(AGENTID,PLAYERID),CONSTRAINT SYS_CT_51 UNIQUE(PLAYER),CONSTRAINT FKE20F7532645EC862 FOREIGN KEY(AGENTID) REFERENCES AGENT(ID))
CREATE MEMORY TABLE DIVISION(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,NAME VARCHAR(255))
CREATE MEMORY TABLE PLAYER(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,NAME VARCHAR(255),CLUB BIGINT,CONSTRAINT FK8EA387017913A34F FOREIGN KEY(CLUB) REFERENCES CLUB(ID))
CREATE MEMORY TABLE SPONSORSHIP(CLUBID BIGINT NOT NULL,COMPANYID BIGINT NOT NULL,DURATION INTEGER NOT NULL,STARTDATE TIMESTAMP,VALUE FLOAT NOT NULL,PRIMARY KEY(CLUBID,COMPANYID),CONSTRAINT FK9A07B83650D79212 FOREIGN KEY(COMPANYID) REFERENCES COMPANY(ID),CONSTRAINT FK9A07B83627FD5AEA FOREIGN KEY(CLUBID) REFERENCES CLUB(ID))
CREATE MEMORY TABLE RESULT(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,AWAYSCORE INTEGER NOT NULL,HOMESCORE INTEGER NOT NULL,AWAYCLUBID BIGINT,HOMECLUBID BIGINT,DATEPLAYED TIMESTAMP,CONSTRAINT FK91B2B43DC4D358 FOREIGN KEY(AWAYCLUBID) REFERENCES CLUB(ID),CONSTRAINT FK91B2B43DC934EC89 FOREIGN KEY(HOMECLUBID) REFERENCES CLUB(ID))
CREATE MEMORY TABLE PRODUCT(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,NAME VARCHAR(255),COMPANY BIGINT,CONSTRAINT FK50C664CFDDB4B037 FOREIGN KEY(COMPANY) REFERENCES COMPANY(ID))
CREATE MEMORY TABLE LICENSINGDEAL(PLAYERID BIGINT NOT NULL,PRODUCTID BIGINT NOT NULL,DURATION INTEGER NOT NULL,STARTDATE TIMESTAMP,VALUE FLOAT NOT NULL,PRIMARY KEY(PLAYERID,PRODUCTID),CONSTRAINT FKBFABA62A1B425E76 FOREIGN KEY(PRODUCTID) REFERENCES PRODUCT(ID),CONSTRAINT FKBFABA62A59C97BC0 FOREIGN KEY(PLAYERID) REFERENCES PLAYER(ID))
CREATE MEMORY TABLE LICENSINGDEAL_AGENT(LICENSINGDEALS_PLAYERID BIGINT NOT NULL,LICENSINGDEALS_PRODUCTID BIGINT NOT NULL,AGENTS_ID BIGINT NOT NULL,PRIMARY KEY(LICENSINGDEALS_PLAYERID,LICENSINGDEALS_PRODUCTID,AGENTS_ID),CONSTRAINT FKB17D94904FF6588D FOREIGN KEY(LICENSINGDEALS_PLAYERID,LICENSINGDEALS_PRODUCTID) REFERENCES LICENSINGDEAL(PLAYERID,PRODUCTID),CONSTRAINT FKB17D94903E46166E FOREIGN KEY(AGENTS_ID) REFERENCES AGENT(ID))
CREATE MEMORY TABLE CLUBSTATS(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,DRAW FLOAT NOT NULL,LOST FLOAT NOT NULL,POINTS FLOAT NOT NULL,WON FLOAT NOT NULL,CLUB BIGINT,CONSTRAINT SYS_CT_89 UNIQUE(CLUB),CONSTRAINT FK7A23D1697913A34F FOREIGN KEY(CLUB) REFERENCES CLUB(ID))
